@model IEnumerable<Biblioteka.PrzetrzymaneKsiazki>

@{
    ViewBag.Title = "PrzetrzymaneKsiazkis";
}

<h2>Lista przetrzymanych egzemplarzy</h2>

<p>

</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdEgzemplarz)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdCzytelnik)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Imie)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nazwisko)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tytul)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IloscDni)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WartoscKary)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdEgzemplarz)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdCzytelnik)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Imie)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nazwisko)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tytul)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IloscDni)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WartoscKary)
            </td>
            <td></td>
        </tr>
    }

</table>

<div>
    @Html.ActionLink("Powrót do listy", "Index")
</div>